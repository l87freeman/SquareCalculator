<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="loggersTypes"
             type="Utils.LoggerConfigurationSection, Utils" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>

  <loggersTypes>
    <add name="FileLogger" type="Logger.FileLogger"/>
    <!--<add name="ConsoleLogger" type="Type2"/>-->
  </loggersTypes>
  
  <appSettings>
    <add key="fileLogPath" value="fileLog"/>
  </appSettings>
</configuration>